// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @id @default(uuid())
  mail String
  pwd String
  nickname String

  @@index([mail])
}

model Alarm {
  id String @id @default(uuid())
  type String // cross | break | pinbar
}
